{
   "version": 2,
   "builds": [
     { "src": "client/**/*", "use": "@vercel/static-build" },
   //   { "src": "server/**/*.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "handle": "filesystem" },
   //   { "src": "/api/.*", "dest": "server/**/*.js" },
     { "src": "/(.*)", "dest": "/client/$1" }
   ]
 } 